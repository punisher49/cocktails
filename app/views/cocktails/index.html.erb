<h1>Cocktails</h1>
<%= link_to "New Cocktail", new_cocktail_path %>

<% @cocktail.each do |cocktail| %>
  <p>
  <%= cocktail.id %>)Name: <%= cocktail.name %>
  </p>

   <p>
   <%= cocktail.image %>
   </p>

   <p>
   Creator: <%= cocktail.creator %>
   </p>

   <p>
   Ingredients: <%= cocktail.ingredients %>
   </p>

   <p>
   Instructions: <%= cocktail.instructions %>
   </p>

   <%= link_to "Show", cocktail %>
   <%= link_to "Edit", edit_cocktail_path(cocktail) %>
   <%= link_to "Delete", delete_cocktail_path(cocktail), method: :delete, data: {confirm: "Are you sure?"} %>
<% end %>
